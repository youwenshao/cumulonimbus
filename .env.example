# Database Configuration
# PostgreSQL connection string
# Format: postgresql://USER:PASSWORD@HOST:PORT/DATABASE
DATABASE_URL="postgresql://user:password@localhost:5432/cumulonimbus?schema=public"

# Authentication Configuration
# Generate a secret with: openssl rand -base64 32
NEXTAUTH_SECRET="your-secret-key-here-generate-with-openssl-rand-base64-32"
# Your application URL (change for production)
NEXTAUTH_URL="http://localhost:3000"

# AI/LLM Configuration (Qwen via OpenRouter)
# Get your API key from: https://openrouter.ai
QWEN_API_KEY="your-openrouter-api-key"
# OpenRouter API endpoint
QWEN_API_URL="https://openrouter.ai/api/v1"
# Model to use (qwen-2.5-coder-32b-instruct recommended)
QWEN_MODEL="qwen/qwen-2.5-coder-32b-instruct"
# Provider: "openrouter" or "puter"
QWEN_PROVIDER="openrouter"
